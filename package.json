    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.6.0",
    "helmet": "^6.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "uuidv4": "^6.2.13",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  },
